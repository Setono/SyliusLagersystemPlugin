{
    "name": "setono/sylius-lagersystem-plugin",
    "type": "sylius-plugin",
    "description": "Official Sylius plugin for the Lagersystem.dk system",
    "keywords": [
        "sylius",
        "sylius-plugin",
        "lagersystem",
        "setono"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.2",
        "sylius/sylius": "^1.7"
    },
    "require-dev": {
        "behat/behat": "^3.4",
        "behat/mink": "^1.7@dev",
        "behat/mink-browserkit-driver": "^1.3",
        "behat/mink-extension": "^2.2",
        "behat/mink-selenium2-driver": "^1.3",
        "ergebnis/composer-normalize": "^2.2",
        "friends-of-behat/page-object-extension": "^0.3",
        "friends-of-behat/suite-settings-extension": "^1.0",
        "friends-of-behat/symfony-extension": "^2.0",
        "friends-of-behat/variadic-extension": "^1.1",
        "korbeil/phpstan-generic-rules": "^0.2.4",
        "lakion/mink-debug-extension": "^1.2.3",
        "lchrusciel/api-test-case": "^4.0",
        "loevgaard/sylius-barcode-plugin": "^1.4",
        "loevgaard/sylius-brand-plugin": "^2.0",
        "matthiasnoback/symfony-config-test": "^4.0",
        "matthiasnoback/symfony-dependency-injection-test": "^4.0",
        "phpspec/phpspec": "^6.1",
        "phpstan/phpstan": "^0.12",
        "phpstan/phpstan-doctrine": "^0.12",
        "phpstan/phpstan-strict-rules": "^0.12",
        "phpstan/phpstan-webmozart-assert": "^0.12",
        "phpunit/phpunit": "^8.0",
        "sensiolabs/security-checker": "^5.0",
        "sylius-labs/coding-standard": "^3.1",
        "symfony/browser-kit": "^3.4 || ^4.3 || ^5.0",
        "symfony/debug-bundle": "^3.4 || ^4.3 || ^5.0",
        "symfony/dotenv": "^4.3 || ^5.0",
        "symfony/intl": "^3.4 || ^4.3 || ^5.0",
        "symfony/web-profiler-bundle": "^4.3 || ^5.0",
        "symfony/web-server-bundle": "^4.3 || ^5.0",
        "thecodingmachine/phpstan-safe-rule": "^1.0"
    },
    "suggest": {
        "loevgaard/sylius-barcode-plugin": "To render barcode at product variant endpoint",
        "loevgaard/sylius-brand-plugin": "To render brand at product variant endpoint"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "autoload": {
        "psr-4": {
            "Setono\\SyliusLagersystemPlugin\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\Setono\\SyliusLagersystemPlugin\\": "tests/"
        },
        "classmap": [
            "tests/Application/Kernel.php"
        ]
    },
    "prefer-stable": true,
    "scripts": {
        "analyse": "vendor/bin/phpstan analyse -c phpstan.neon -l max --no-progress src/ tests/Controller",
        "check-style": "vendor/bin/ecs check --no-progress-bar src/ tests/Controller",
        "fix-style": "vendor/bin/ecs check src/ tests/Controller --fix",
        "phpspec": "vendor/bin/phpspec run -vvv",
        "phpunit": "vendor/bin/phpunit",
        "tests": [
            "@phpspec",
            "@phpunit"
        ]
    }
}
